<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.leyi.gmall.pms.mapper.ProductCategoryMapper">

    <select id="selectListWithChildren" resultType="org.leyi.gmall.mbg.pms.dto.PmsProductMultiCategoryDto">
        SELECT
            `id`,
            `parent_id`,
            `name`,
            `level`,
            `product_count`,
            `nav_status`,
            `show_status`,
            `sort`,
            `icon`,
            `keywords`,
            `description`
        FROM
            pms_product_category
    </select>

</mapper>
